{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<div id="site-navigation">
  <div class="container">
    <div class="more_pc">
      {% if settings.logo_image_sticky %}
      <div class="logo_sticky">
        <a href="{{langify_shop_url}}" title="{{ langify_shop_name | escape }}">
          <img src="{% comment %}ly_asset_replace_for_['logo_sticky.png' | asset_url]_begin{% endcomment %}{% assign ly_asset = 'logo_sticky.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" alt="" />
        </a>
      </div>
      {% endif %}
      {% if settings.nav_linklist != '' %}
      <nav class="pc_navbar" role="navigation">
        <ul class="pc_nav">
          {% for link in linklists[settings.nav_linklist].links %}
          {% include 'navigation-link' with 'top' %}
          {% endfor %}
          <li class="nav-menu-icon visible-xs">
            <a class="btn-navbar" data-toggle="offcanvas">
              <span>
                <span>
                  <i class="icon-bar"></i>
                  <i class="icon-bar"></i>
                  <i class="icon-bar"></i>
                </span>
              </span>
            </a>
          </li>
        </ul>
      </nav>
      {% endif %}
      <div class="pc_more">
       
       
        <div class="mini-cart nav-dropdown-parent">
          <div class="cart-inner">
            <div class="cart-link">
              <div class="cart-icon">
                <strong>{{cart.item_count}}</strong>
                <span class="cart-icon-handle"></span>
              </div>
            </div>
          </div>
          <div class="nav-dropdown">
            <div class="nav-dropdown-inner">
              {% include 'cart-sidebar' %}
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>