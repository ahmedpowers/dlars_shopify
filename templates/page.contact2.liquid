{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<div id="col-main" class="contact_map clearfix">
  <div id="content" class="container">
    <div class="row">
      <div class="col-md-4 col-sm-12">
        <div class="widget-wrapper">
          {% form 'contact' %}

          {% if settings.contact_map_heading != blank %}
          <h2 class="title">
            {% comment %}ly_new_settings_replace_for_[ settings.contact_map_heading ]_begin{% endcomment %}{% if request.host %}{% assign host = request.host %}{% assign domains = shop.metafields.language_domains %}{% for domain in domains %}{% assign key = domain | first %}{% assign value = domain | last | split: ',' %}{% if value contains host %}{% assign language = key %}{% endif %}{% endfor %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% else %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% endif %}{% assign namespace = language | append: "as" %}{% if shop.metafields[namespace]["id2712149"] %}{% assign ly_translation = shop.metafields[namespace]["id2712149"] %}{% else %}{% assign ly_translation = settings.contact_map_heading %}{% endif %}{{ ly_translation }}{% comment %}ly_new_settings_replace_end{% endcomment %}
          </h2>
          {% endif %}

          {% if form.posted_successfully? %}
          <div class="alert alert-success">
            <button type="button" class="close" data-dismiss="alert">×</button>
            {{ 'contact.form.post_success' | t }}
          </div>
          {% endif %}

          {% if form.errors %}
          <div class="alert alert-error">
            <button type="button" class="close" data-dismiss="alert">×</button>
            {{ form.errors | default_errors }}
          </div>
          {% endif %}

          <ul id="contact-form" class="list-unstyled">
            <li>
              <div class="group-input">      
                <input required="required" type="text" id="name" value="{% if form.name %}{{ form.name }}{% elsif customer %}{{ customer.first_name }} {{ customer.last_name }}{% endif %}" name="contact[name]" />
                <span class="highlight"></span>
                <span class="bar"></span>
                <label>{{ 'contact.form.name' | t }} *</label>
              </div>
            </li>
            <li>
              <div class="group-input">      
                <input required="required" type="email" id="email" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}" name="contact[email]" />
                <span class="highlight"></span>
                <span class="bar"></span>
                <label>{{ 'contact.form.email' | t }} *</label>
              </div>
            </li>
            <li>
              <div class="group-input">      
                <input type="text" id="phone" value="{{ form.phone }}" name="contact[phone]" />
                <span class="highlight"></span>
                <span class="bar"></span>
                <label>{{ 'contact.form.phone' | t }}</label>
              </div>
            </li>
            <li>
              <div class="group-input">      
                <textarea required="required" id="message" cols="40" rows="5" class="input" name="contact[body]">{{ form.body }}</textarea>
                <span class="highlight"></span>
                <span class="bar"></span>
                <label>{{ 'contact.form.message' | t }} *</label>
              </div>
            </li>
            <li class="last">
              <button type="submit" class="ripple btn btn-1">
                {{ 'contact.form.send' | t }}
              </button>
            </li>
          </ul>
          {% endform %}
        </div>
      </div>
      <div class="col-md-4 col-sm-6">
        <div id="contact_map" class="map"></div>
      </div>
      <div class="col-md-4 col-sm-6">
        {% if settings.contact_info_show %}
        <div class="widget-contact widget-wrapper">
          {% if settings.contact_info_heading != blank %}
          <div class="title">
            {% comment %}ly_new_settings_replace_for_[ settings.contact_info_heading ]_begin{% endcomment %}{% if request.host %}{% assign host = request.host %}{% assign domains = shop.metafields.language_domains %}{% for domain in domains %}{% assign key = domain | first %}{% assign value = domain | last | split: ',' %}{% if value contains host %}{% assign language = key %}{% endif %}{% endfor %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% else %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% endif %}{% assign namespace = language | append: "as" %}{% if shop.metafields[namespace]["id2712150"] %}{% assign ly_translation = shop.metafields[namespace]["id2712150"] %}{% else %}{% assign ly_translation = settings.contact_info_heading %}{% endif %}{{ ly_translation }}{% comment %}ly_new_settings_replace_end{% endcomment %}
          </div>
          {% endif%}
          <ul class="list-unstyled contact-world-map">
            {% if settings.contact_info_address != blank %}
            <li>
              <i class="fa fa-home"></i>&nbsp; {% comment %}ly_new_settings_replace_for_[ settings.contact_info_address ]_begin{% endcomment %}{% if request.host %}{% assign host = request.host %}{% assign domains = shop.metafields.language_domains %}{% for domain in domains %}{% assign key = domain | first %}{% assign value = domain | last | split: ',' %}{% if value contains host %}{% assign language = key %}{% endif %}{% endfor %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% else %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% endif %}{% assign namespace = language | append: "as" %}{% if shop.metafields[namespace]["id7405735"] %}{% assign ly_translation = shop.metafields[namespace]["id7405735"] %}{% else %}{% assign ly_translation = settings.contact_info_address %}{% endif %}{{ ly_translation }}{% comment %}ly_new_settings_replace_end{% endcomment %}
            </li>
            {% endif %}

            {% if settings.contact_info_phone != blank %}
            <li>
              <i class="fa fa-phone"></i>&nbsp; {% comment %}ly_new_settings_replace_for_[ settings.contact_info_phone ]_begin{% endcomment %}{% if request.host %}{% assign host = request.host %}{% assign domains = shop.metafields.language_domains %}{% for domain in domains %}{% assign key = domain | first %}{% assign value = domain | last | split: ',' %}{% if value contains host %}{% assign language = key %}{% endif %}{% endfor %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% else %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% endif %}{% assign namespace = language | append: "as" %}{% if shop.metafields[namespace]["id7405736"] %}{% assign ly_translation = shop.metafields[namespace]["id7405736"] %}{% else %}{% assign ly_translation = settings.contact_info_phone %}{% endif %}{{ ly_translation }}{% comment %}ly_new_settings_replace_end{% endcomment %}
            </li>
            {% endif %}

            {% if settings.contact_info_email != blank %}
            <li>
              <i class="fa fa-envelope"></i>&nbsp; <a href="mailto:{% comment %}ly_new_settings_replace_for_[ settings.contact_info_email ]_begin{% endcomment %}{% if request.host %}{% assign host = request.host %}{% assign domains = shop.metafields.language_domains %}{% for domain in domains %}{% assign key = domain | first %}{% assign value = domain | last | split: ',' %}{% if value contains host %}{% assign language = key %}{% endif %}{% endfor %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% else %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% endif %}{% assign namespace = language | append: "as" %}{% if shop.metafields[namespace]["id7405737"] %}{% assign ly_translation = shop.metafields[namespace]["id7405737"] %}{% else %}{% assign ly_translation = settings.contact_info_email %}{% endif %}{{ ly_translation }}{% comment %}ly_new_settings_replace_end{% endcomment %}">{% comment %}ly_new_settings_replace_for_[ settings.contact_info_email ]_begin{% endcomment %}{% if request.host %}{% assign host = request.host %}{% assign domains = shop.metafields.language_domains %}{% for domain in domains %}{% assign key = domain | first %}{% assign value = domain | last | split: ',' %}{% if value contains host %}{% assign language = key %}{% endif %}{% endfor %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% else %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% endif %}{% assign namespace = language | append: "as" %}{% if shop.metafields[namespace]["id7405737"] %}{% assign ly_translation = shop.metafields[namespace]["id7405737"] %}{% else %}{% assign ly_translation = settings.contact_info_email %}{% endif %}{{ ly_translation }}{% comment %}ly_new_settings_replace_end{% endcomment %}</a>
            </li>
            {% endif %}

            {% if settings.contact_info_other != blank %}
            <li>
              <i class="fa fa-clock-o"></i>&nbsp; {{ settings.contact_info_other }}
            </li>
            {% endif %}
          </ul>
        </div>
        {% endif %}

        {% if settings.contact_social_show %}
        <div class="widget-wrapper social-icons">
          {% if settings.contact_social_heading != blank %}
          <div class="title">
            {% comment %}ly_new_settings_replace_for_[ settings.contact_social_heading ]_begin{% endcomment %}{% if request.host %}{% assign host = request.host %}{% assign domains = shop.metafields.language_domains %}{% for domain in domains %}{% assign key = domain | first %}{% assign value = domain | last | split: ',' %}{% if value contains host %}{% assign language = key %}{% endif %}{% endfor %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% else %}{% assign language = cart.attributes["language"] %}{% if language == nil %}{% assign language = shop.metafields["languages"]["default"] %}{% endif %}{% endif %}{% assign namespace = language | append: "as" %}{% if shop.metafields[namespace]["id2712155"] %}{% assign ly_translation = shop.metafields[namespace]["id2712155"] %}{% else %}{% assign ly_translation = settings.contact_social_heading %}{% endif %}{{ ly_translation }}{% comment %}ly_new_settings_replace_end{% endcomment %}
          </div>
          {% endif %}

          <ul class="list-inline text-left">
            {% if settings.contact_social_facebook_show %}<li><a target="_blank" href="{{ settings.social_icons_facebook_link }}" class="social-icon facebook" title="<ly-as-2712156>Facebook</ly-as-2712156>"><i class="fa fa-facebook"></i></a></li>{% endif %}
            {% if settings.contact_social_twitter_show %}<li><a target="_blank" href="{{ settings.social_icons_twitter_link }}" class="social-icon twitter" title="<ly-as-2712157>Twitter</ly-as-2712157>"><i class="fa fa-twitter"></i></a></li>{% endif %}
            {% if settings.contact_social_pinterest_show %}<li><a target="_blank" href="{{ settings.social_icons_pinterest_link }}" class="social-icon pinterest" title="<ly-as-2712158>Pinterest</ly-as-2712158>"><i class="fa fa-pinterest"></i></a></li>{% endif %}
            {% if settings.contact_social_google_show %}<li><a target="_blank" href="{{ settings.social_icons_google_link }}" class="social-icon google" title="<ly-as-2712159>Google Plus</ly-as-2712159>"><i class="fa fa-google-plus"></i></a></li>{% endif %}
            {% if settings.contact_social_youtube_show %}<li><a target="_blank" href="{{ settings.social_icons_youtube_link }}" class="social-icon youtube" title="<ly-as-2712160>Youtube</ly-as-2712160>"><i class="fa fa-youtube"></i></a></li>{% endif %}
            {% if settings.contact_social_vimeo_show %}<li><a target="_blank" href="{{ settings.social_icons_vimeo_link }}" class="social-icon vimeo" title="<ly-as-2712161>Vimeo</ly-as-2712161>"><i class="fa fa-vimeo-square"></i></a></li>{% endif %}
            {% if settings.contact_social_flickr_show %}<li><a target="_blank" href="{{ settings.social_icons_vimeo_link }}" class="social-icon flickr" title="<ly-as-2712162>Flickr</ly-as-2712162>"><i class="fa fa-flickr"></i></a></li>{% endif %}
            {% if settings.contact_social_instagram_show %}<li><a target="_blank" href="{{ settings.social_icons_instagram_link }}" class="social-icon instagram" title="<ly-as-2712163>Instagram</ly-as-2712163>"><i class="fa fa-instagram"></i></a></li>{% endif %}
            {% if settings.contact_social_tumblr_show %}<li><a target="_blank" href="{{ settings.social_icons_tumblr_link }}" class="social-icon tumblr" title="<ly-as-2712164>Tumblr</ly-as-2712164>"><i class="fa fa-tumblr"></i></a></li>{% endif %}
            {% if settings.contact_social_rss_show %}<li><a target="_blank" href="{{ settings.social_icons_rss_link }}" class="social-icon rss" title="<ly-as-2712165>RSS Feed</ly-as-2712165>"><i class="fa fa-rss"></i></a></li>{% endif %}
          </ul>
        </div>
        {% endif %}
      </div>
    </div>
  </div>
</div>